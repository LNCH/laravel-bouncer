{
    "name": "lnch/laravel-bouncer",
    "description": "Laravel package for controlling permissions and roles for Eloquent models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lynch",
            "email": "tom@lnch.co.uk"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Lnch\\LaravelBouncer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lnch\\LaravelBouncer\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "5.0",
        "phpunit/phpunit": "^8.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lnch\\LaravelBouncer\\LaravelBouncerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "/vendor/bin/phpunit",
        "test-f": "/vendor/bin/phpunit --filter"
    }
}
